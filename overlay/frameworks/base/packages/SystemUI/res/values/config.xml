<resources>
    <!-- Doze: does this device support STATE_DOZE and STATE_DOZE_SUSPEND?  -->
    <bool name="doze_display_state_supported">true</bool>

    <!-- Max visible notification icons -->
    <integer name="config_maxVisibleNotificationIcons">6</integer>
    <integer name="config_maxVisibleNotificationIconsWhenLock">7</integer>

    <!-- StatusBar burn-in protection -->
    <bool name="config_statusBarBurnInProtection">false</bool>

    <!-- The CPUs to display in CPUInfoService view -->
    <string name="config_displayCpus" translatable="false">0,1,2,3</string>

    <!-- The battery current divider, some kernel report current value in mA other in ÂµA.
         Leave 1 if you have mA, put 1000 if you have ÂµA -->
    <integer name="config_battCurrentDivider" translatable="false">1000</integer>

    <!-- If not zero, limits the internal screen recorder's framerate to the set value. -->
    <integer name="config_screenRecorderMaxFramerate">30</integer>
</resources>
